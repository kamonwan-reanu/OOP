import java.util.Scanner;

public class InventoryManagement {
    
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        
        int totalProducts = input.nextInt();

        int[] productIds = new int[totalProducts];
        int[] stockQuantities = new int[totalProducts];

        for (int i = 0; i < totalProducts; i++) {
            productIds[i] = input.nextInt();
            stockQuantities[i] = input.nextInt();
        }

        int searchId = input.nextInt();

        int foundIndex = -1;
        for (int i = 0; i < totalProducts; i++) {
            if (productIds[i] == searchId) {
                foundIndex = i;
                break;
            }
        }

        if (foundIndex != -1) {
            System.out.println(stockQuantities[foundIndex]);
        } else {
            System.out.println("Product " + searchId + " not found");
        }
        
        input.close();
    }
}
