import java.util.Scanner;

public class ServerMonitoringDashboard {

    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);

        int totalRows = input.nextInt();
        int totalColumns = input.nextInt();

        int[][] serverStatus = new int[totalRows][totalColumns];

        for (int row = 0; row < totalRows; row++) {
            String statusLine = input.next();
            
            for (int col = 0; col < totalColumns; col++) {
                char statusChar = statusLine.charAt(col);
                serverStatus[row][col] = Character.getNumericValue(statusChar);
            }
        }

        int onlineCount = 0;
        
        for (int row = 0; row < totalRows; row++) {
            for (int col = 0; col < totalColumns; col++) {
                if (serverStatus[row][col] == 1) {
                    onlineCount++;
                }
            }
        }

        System.out.println(onlineCount);
        
        input.close();
    }
}
